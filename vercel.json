{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/dist/$1",
        "continue": true
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "status": 404,
        "dest": "/frontend/dist/index.html"
      }
    ]
  }